    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000001 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000001">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource that only consists of a set of terms and their definitions, possibly including synonyms.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:37:33Z</oboInOwl:creation_date>
        <rdfs:comment xml:lang="en">Examples: NSIDC&apos;s Cryosphere Glossary https://nsidc.org/cryosphere/glossary</rdfs:comment>
        <rdfs:label xml:lang="en">Glossary</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000002 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000002">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource consisting of a structured controlled vocabulary where there is information about each term and its hierarchical, associative, or equivalence relationships to other terms in the thesaurus.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:40:05Z</oboInOwl:creation_date>
        <rdfs:label xml:lang="en">Thesaurus</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000003 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000003">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource containing a set of concepts with attributes and relationships between the concepts which together define a domain of knowledge, expressed in machine-readable format.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:41:32Z</oboInOwl:creation_date>
        <rdfs:label xml:lang="en">Ontology</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000004 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000004">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource consisting of a list of terms controlled by some authority, generally intended to populate pick lists in user interfaces.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:42:17Z</oboInOwl:creation_date>
        <rdfs:comment xml:lang="en">Assumption is that users will all understand the terms without any additional guidance.</rdfs:comment>
        <rdfs:label xml:lang="en">Controlled Vocabulary</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000005 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000005">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource made up of hierarchies of terms organized as tree structures without significant relationships to concepts in other trees.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:43:21Z</oboInOwl:creation_date>
        <rdfs:comment xml:lang="en">Example: domain, kingdom, phylum or division, class, genus, species is the taxonomic heirarchy in Biology.</rdfs:comment>
        <rdfs:label xml:lang="en">Taxonomy</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000006 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000006">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource consisting of an information model represented with artifacts such as diagrams (e.g. UML, concept maps) and associated text, not formalized as an ontology.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:49:05Z</oboInOwl:creation_date>
        <rdfs:label xml:lang="en">Conceptual Model</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000007 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000007">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource containing a representation of a knowledge domain that defines rdf classes and attributes, along with domain and range constraints for some attributes.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:50:36Z</oboInOwl:creation_date>
        <oboInOwl:hasExactSynonym xml:lang="en">Resource Description Framework vocabulary</oboInOwl:hasExactSynonym>
        <rdfs:comment xml:lang="en">Distinguish from more formal ontologies by absence of general class relationships, and restrictions offered by representations like OWL.</rdfs:comment>
        <rdfs:label xml:lang="en">RDF vocabulary</rdfs:label>
    </owl:Class>
    


    <!-- http://cor.esipfed.org/ont/earthcube/srt_0000008 -->


    <owl:Class rdf:about="http://cor.esipfed.org/ont/earthcube/srt_0000008">
        <rdfs:subClassOf rdf:resource="http://cor.esipfed.org/ont/earthcube/srt_0000009"/>
        <obo:IAO_0000115 xml:lang="en">A semantic resource comprised of a collection of concepts with identifiers, preferred labels, and definitions; including broader, narrower, related and transitive relationships, labels in alternate languages, and other annotation properties.</obo:IAO_0000115>
        <oboInOwl:created_by rdf:resource="http://orcid.org/0000-0003-4808-4736"/>
        <oboInOwl:creation_date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-10-22T04:52:45Z</oboInOwl:creation_date>
        <rdfs:comment xml:lang="en">Encoded using SKOS rdf vocabulary.</rdfs:comment>
        <rdfs:comment xml:lang="en">Glossary, Taxonomy,  and Thesaurus might be represented using SKOS vocabulary, but each lack one or more of the semantic enhancments offered by SKOS.</rdfs:comment>
        <rdfs:label xml:lang="en">SKOS vocabulary</rdfs:label>
    </owl:Class>
    


